#POCKET_GRID_FILE[1]			/orange/yaxia.yuan/work/dud/all/package/input/receptor_grid_1.txt
#PROTEIN_FILE[1] 			/orange/yaxia.yuan/work/dud/all/package/input/receptor.pdb
POCKET_ATOM_FILE[1]                     /orange/yaxia.yuan/work/dud/all/egfr/receptor_pocket_2.txt
POCKET_GRID_FILE[1]                     /orange/yaxia.yuan/work/dud/all/egfr/receptor_grid_2.txt
PROTEIN_FILE[1]                         /orange/yaxia.yuan/work/dud/all/egfr/receptor.pdb
