SCORE_OPTIMIZE                          YES
#OPTFLAGSCORE_LOG                               /ufrc/yaxia.yuan/yaxia.yuan/dud/all/MARK/LIGANDTYPE.score_opt
ADD_HSPC    NO
SEED_DIAGNOSE                           YES
EC_DIAGNOSE                           YES
